{
  "manifest_version": 2,
  "name": "CTMail Launcher",
  "description": "Quick launcher by Vu Chuc",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://ctmail.vnu.edu.vn/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
